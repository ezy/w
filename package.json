{
  "name": "flat-file-node",
  "version": "0.0.0",
  "private": true,
  "author": "ezy",
  "scripts": {
    "start": "nodemon ./bin/www",
    "docker:run": "./bin/docker-start.sh"
  },
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@hapi/joi-date": "^1.3.0",
    "body-parser": "^1.19.0",
    "change-case": "^3.1.0",
    "cookie-parser": "~1.3.5",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.4",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "no-param-reassign": 0,
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  }
}
